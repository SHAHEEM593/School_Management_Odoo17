        <field name="url">/event_tree</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">30</field>
    </record>
    <!--    leave tree-->
    <record id="website_menu_leave_tree" model="website.menu">
        <field name="name">Leaves</field>
        <field name="url">/leave_tree</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">30</field>
    </record>
</odoo>
